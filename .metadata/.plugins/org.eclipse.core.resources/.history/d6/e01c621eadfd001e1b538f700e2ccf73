package variaveis;

public class ConhecerVariaveis {

	public static void main(String[] args) {
		// variáveis Java
		
		/*
		 * quando vc tiver um comentário 
		 * muito longo
		 * de várias linhas
		 */
		
		int numero;
		
		numero = 10;
		
		int x = 20;
		
		System.out.println("O número é: " + numero);

	}

}
