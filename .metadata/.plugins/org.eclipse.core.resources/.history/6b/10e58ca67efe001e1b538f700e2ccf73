package operadores;

public class operadores {

	public static void main(String[] args) {
		// vamos testar os operadores
		
		//
		int a,b,resultado;
		a=25;
		b=45;
		resultado = a + b;
		
		System.out.println(resultado);

		resultado = b - a;
		
		System.out.println(resultado);
		
		//multiplicação
		
		resultado = a *2;
		System.out.println(resultado);
		
		float resultado2, num1,num2;
		num1 = 25.0f;
		num2 = 45.0f;
		//divisão
		
		System.out.println(resultado2);
	}

}
